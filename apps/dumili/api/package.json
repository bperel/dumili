{
  "name": "~dumili-api",
  "type": "module",
  "version": "1.0.0",
  "engines": {
    "node": ">=18.0.0 <19.0.0"
  },
  "scripts": {
    "clean": "rm -rf dist",
    "build": "rm -rf dist/{api,types} && ./node_modules/.bin/tsc && tsc-alias",
    "dev": "concurrently -n typecheck,tsx \"tsc --noEmit\" \"tsx watch index.ts\"",
    "lint": "eslint --fix .",
    "prisma": "prisma db pull && prisma generate"
  },
  "dependencies": {
    "@prisma/client": "^5.10.2",
    "@sentry/node": "^7.100.1",
    "axios": "^1.6.7",
    "body-parser": "^1.20.2",
    "busboy": "^1.6.0",
    "cloudinary": "^1.41.3",
    "connect-busboy": "^1.0.0",
    "cors": "^2.8.5",
    "dotenv": "^16.4.1",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.2",
    "pdf2pic": "^3.1.1",
    "sharp": "^0.33.2",
    "socket.io": "^4.7.4",
    "~socket.io-services": "workspace:*",
    "~socket.io-client-services": "workspace:*"
  },
  "devDependencies": {
    "@socket.io/admin-ui": "^0.5.1",
    "@types/body-parser": "^1.19.5",
    "@types/busboy": "^1.5.3",
    "@types/connect-busboy": "^1.0.3",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/i18n": "^0.13.10",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^16.18.80",
    "concurrently": "^8.2.2",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "eslint-plugin-unused-imports": "^3.0.0",
    "prisma": "^5.10.2",
    "tsx": "^4.7.1",
    "typescript": "^5.3.3",
    "typescript-eslint": "^7.0.1"
  }
}