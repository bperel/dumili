    "dayjs": "^1.11.8",
    "ducksmanager": "^0.0.61",
    "js-cookie": "^3.0.5",
    "page-flip": "^2.0.7",
    "pinia": "^2.1.4",
    "vue": "^3.3.4",
    "vue-i18n": "^9.3.0-beta.22",
    "vue-router": "^4.2.2"
